{
    "name": "mrtool/contao-jqueryfancybox",
    "description": "Jquery Fancybox for Contao 3.1",
    "keywords": ["jquery", "fancybox", "contao"],
    "type": "contao-module",
    "homepage": "http://www.r2pi.de",
    "license": [
        "LGPL-3.0+",
        "CC-BY-NC-3.0"
    ],
    "authors": [
        {
            "name": "Martin Treml",
            "homepage": "http://www.r2pi.de",
            "role": "Developer - Wrapper"
        },
        {
            "name": "Janis Skarnelis",
            "homepage": "http://www.fancyapps.com",
            "role": "Developer - Fancybox"
        }
    ],
    "support": {
        "issues": "https://github.com/MrTool/contao-jqueryFancybox/issues",
        "irc": "irc://irc.freenode.org/contao.dev",
        "source": "https://github.com/MrTool/contao-jqueryFancybox",
        "forum": "https://community.contao.org/de/forumdisplay.php?119-Sonstige-Erweiterungen"
    },
    "require": {
        "php": ">=5.3",
        "contao/core": ">=3,<4",
        "contao-community-alliance/composer-plugin": "~2.0"
    },
    "autoload": {
        "psr-0": {
            "MrTool\\fancyBox": "src"
        }
    },
    "replace": {
        "contao-legacy/fancybox": "*"
    },
    "extra": {
        "contao": {
            "sources": {
                "assets/jquery/fancybox": "assets/jquery/fancybox",
                "contao": "system/modules/jqueryFancybox"
            }
        }
    }
}
